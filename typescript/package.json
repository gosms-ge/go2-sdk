{
  "name": "@go2ge/sdk",
  "version": "1.2.0",
  "description": "Official TypeScript/Node.js SDK for the Go2 gRPC API",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "proto"
  ],
  "scripts": {
    "generate": "bash ../scripts/generate-typescript.sh",
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "keywords": [
    "go2",
    "smart-links",
    "grpc",
    "api",
    "sdk",
    "integrations"
  ],
  "author": "Go2 <support@go2.ge>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gosms-ge/go2-sdk"
  },
  "homepage": "https://developers.go2.ge",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.10.0",
    "@grpc/proto-loader": "^0.7.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
